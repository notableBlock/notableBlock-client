{
  "compilerOptions": {
    "target": "esnext",
    "lib": ["dom", "dom.Iterable", "esnext"],
    "jsx": "react-jsx",
    "module": "esnext",
    "moduleResolution": "bundler",
    "baseUrl": "src",
    "paths": {
      "assets/*": ["./assets/*"],
      "components/*": ["./components/*"],
      "hooks/*": ["./hooks/*"],
      "pages/*": ["./pages/*"],
      "services/*": ["./services/*"],
      "stores/*": ["./stores/*"],
      "styles/*": ["./styles/*"],
      "types/*": ["./types/*"],
      "utils/*": ["./utils/*"],
      "src/constants": ["./constants"]
    },
    "allowJs": true,
    "checkJs": true,
    "declaration": true,
    "noEmit": true,
    "removeComments": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noFallthroughCasesInSwitch": true,
    "skipLibCheck": true,
    "isolatedModules": true
  },
  "include": ["src", "vite-env.d.ts"]
}
